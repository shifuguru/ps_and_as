{
  "compilerOptions": {
    "jsx": "react-native",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "esnext"
    ],
    "types": [
      "react",
      "react-native",
      "node"
    ]
  },
  "extends": "expo/tsconfig.base"
}
